{
  "name": "teton-tracker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.tsx",
  "module": "src/index.tsx",
  "scripts": {
    "dev": "./scripts/dev.sh",
    "dev:api": "bun run src/api-server.ts",
    "dev:frontend": "bun --hot index.html",
    "dev:cleanup": "./scripts/cleanup.sh",
    "build": "bun run build.ts",
    "start": "bun run src/prod-server.ts",
    "preview": "bun run src/prod-server.ts",
    "production-check": "./scripts/production-check.sh",
    "typecheck": "bunx tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "generate-social-preview": "bun run scripts/generate-social-preview.ts",
    "setup-db": "bun run scripts/setup-db.ts",
    "cleanup-db": "bun run scripts/cleanup-db.ts",
    "verify-db": "bun run scripts/verify-db.ts",
    "migrate-db": "bun run scripts/migrate-db.ts",
    "migrate": "bun run scripts/migrate-db.ts migrate",
    "rollback": "bun run scripts/migrate-db.ts rollback",
    "migration-status": "bun run scripts/migrate-db.ts status",
    "create-migration": "bun run scripts/migrate-db.ts create"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.34.0",
    "@clerk/clerk-sdk-node": "^5.1.6",
    "@clerk/themes": "^2.3.3",
    "@hookform/resolvers": "^4.1.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@tanstack/react-router": "^1.127.3",
    "bun-plugin-tailwind": "^0.0.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^12.23.6",
    "libphonenumber-js": "^1.12.10",
    "lucide-react": "^0.475.0",
    "next-themes": "^0.4.6",
    "pg": "^8.12.0",
    "puppeteer": "^24.12.1",
    "react": "^19",
    "react-day-picker": "^9.8.0",
    "react-dom": "^19",
    "react-hook-form": "^7.54.2",
    "sonner": "^2.0.6",
    "svix": "^1.69.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^4.0.6",
    "tailwindcss-animate": "^1.0.7",
    "twilio": "^5.7.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@tanstack/router-devtools": "^1.127.3",
    "@types/bun": "latest",
    "@types/pg": "^8.11.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2"
  }
}
